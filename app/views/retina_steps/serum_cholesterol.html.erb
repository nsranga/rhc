

<div id="content">

  <div class="login">

    <%= form_for @retina, url: wizard_path do |f| %>

    <br><br>
    <center>
      <b><%= f.label :lipid_profile, "Serum Cholesterol", :class => 'lblcss'  %></b>
      <%= f.text_field :lipid_profile, placeholder: 'Serum Cholesterol', :class => 'txtcss'  %>
    </center>
    <br><br>

    <%=  link_to 'Back', previous_wizard_path, :class => 'previousbtn' %>
    <%= f.submit 'Next', :class => 'nextbtn'  %>


  </div>

  <div class="footercontent">
    <br>
    <center><h3><b> <%= f.label :clinical_params, "Clinical Parameters" %></h3>
    <%= f.label :disp_clinic_params, display_clinical_params(@retina) %></b></center>
  </div>

</div>

<% end %>
