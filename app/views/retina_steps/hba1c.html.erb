
<div id="content">
  <div class="login">


<%= form_for @retina, url: wizard_path do |f| %>
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
 <%= @retina.errors.full_messages.to_sentence %>

 <br>
<center>
    <b><%= f.label :hba1c, "HbA1c", :class => 'lblcss' %></b><br>
    <%= f.text_field :hba1c, placeholder: 'Enter HbA1c', :class => 'txtcss' %>
</center>
<br>

        <%=  link_to 'Back', new_retina_path, :class => 'previousbtn' %>

        <%= f.submit 'Next', :class => 'nextbtn' %>
<% end %>


</div>
</div>
